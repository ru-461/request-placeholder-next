{
  "extends": ["eslint:recommended", "next/core-web-vitals", "prettier"],
  "globals": {
    "React": true
  },
  "plugins": ["import", "unused-imports"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "off",
    "react/jsx-sort-props": "warn",
    "unused-imports/no-unused-imports": "warn"
  }
}
